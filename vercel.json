{
	"version": 2,
	"builds": [
		{
			"src": "dist/wharmy/browser/**/*",
			"use": "@vercel/static"
		},
		{
			"src": "api/**",
			"use": "@vercel/node"
		}
	],
	"outputDirectory": "dist/wharmy/browser/browser",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/dist/wharmy/browser/browser/$1"
		},
		{
			"source": "/api/(.*)",
			"destination": "/api/$1"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://wharmyy.vercel.app, https://wh-v.vercel.app"
				},
				{ "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
				{ "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Authorization, X-Api-Version" },
				{ "key": "Access-Control-Allow-Credentials", "value": "true" }
			]
		}
	]
}
